{
	"version": 2,
	"name": "Juninfolio",
	"builds": [
		{
			"src": "src/index.js",
			"use": "@vercel/node"
		},
		{
			"src": "public/**/*",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/styles/(.*)",
			"dest": "/public/styles/$1"
		},
		{
			"src": "/scripts/(.*)",
			"dest": "/public/scripts/$1"
		},
		{
			"src": "/images/(.*)",
			"dest": "/public/images/$1"
		},
		{
			"src": "/(.*)",
			"dest": "src/index.js"
		}
	]
}